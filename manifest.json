{
	"manifest_version": 3,
	"name": "APK-Värde",
	"version": "0.0.1",

	"description": "Beräkna APK-Värdet för Systembolaget.",
	"content_scripts": [
		{
			"matches": ["https://*.systembolaget.se/produkt/*"],
			"js": ["src/apk-content.js"]
		}
	],
	"icons": {
		"16": "icons/16x16.png",
		"48": "icons/48x48.png",
		"128": "icons/128x128.png",
		"512": "icons/512x512.png",
		"1280": "icons/1280x1280.png"
	}
}